#!/usr/bin/env mkb
# Builder mkb file for the OneSignal extension on android
platform LINUX

buildtype (SCONS, ARM)

define S3E_ANDROID

options
{
    dll
    output-name="lib/android/libs3eOneSignal.so"
    output-name-debug="lib/android/libs3eOneSignal_d.so"
}
define S3E_BUILD_EXT

subproject s3eOneSignal_build
